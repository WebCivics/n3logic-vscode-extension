{
  "name": "n3logic-vscode-extension",
  "displayName": "N3Logic VSCode Extension",
  "description": "Language support and semantic tokens for N3Logic.",
  "version": "0.0.1",
  "publisher": "your-publisher",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "n3logic",
        "aliases": [
          "N3Logic",
          "n3logic"
        ],
        "extensions": [
          ".n3",
          ".n3logic"
        ],
        "configuration": "../language-configuration.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37"
  },
  "scripts": {
    "compile": "tsc -p ."
  }
}
